<Window x:Class="Leosac.KeyManager.Library.Wizard.SAMAccessControl.SAMAccessControlWizardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Leosac.KeyManager.Library.Wizard.SAMAccessControl"
        xmlns:properties="clr-namespace:Leosac.KeyManager.Library.Wizard.SAMAccessControl.Properties"
        mc:Ignorable="d"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="{DynamicResource MaterialDesignPaper}"
        FontFamily="{DynamicResource MaterialDesignFont}"
        Title="{x:Static properties:Resources.FactoryName}" Height="450" Width="800" WindowStartupLocation="CenterOwner">
    <StackPanel Orientation="Vertical">
        <TextBlock Text="{x:Static properties:Resources.WizardDescription}" Margin="5" />
    </StackPanel>
</Window>
